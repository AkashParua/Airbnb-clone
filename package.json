{
  "name": "airbnb-clone",
  "version": "1.0.0",
  "description": "\"clone for airbnb website\"",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all -p start:frontend start:backend",
    "start:frontend": "cross-env PORT=3000 --prefix public run dev",
    "start:admin": "npm --prefix admin-ui start",
    "start:backend": "npm --prefix server start",
    "postinstall": "npm -i --prefix public && npm -i --prefix server && npm -i --prefix admin-ui"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkashParua/Airbnb-clone.git"
  },
  "author": "Akash Parua",
